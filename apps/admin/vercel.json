{
  "installCommand": "npx pnpm@9.15.4 install --no-frozen-lockfile",
  "crons": [
    {
      "path": "/api/cron/check-schedules",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/check-pre-notifications",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/apply-pending-agent-downgrades",
      "schedule": "0 * * * *"
    }
  ]
}
